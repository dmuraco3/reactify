{
   "name": "reactify",
   "description": "This extension is made for demonstration purposes",
   "version": "1.0",
   "manifest_version": 3,
   "action": {
      "default_popup": "popup.html"
   },
   "permissions": [
      "activeTab",
      "scripting",
      "clipboardWrite",
      "tabs"
   ],
   "background": {
      "service_worker": "background.js"
   },
   "host_permissions": [
       "*://*/"
   ],
}
